# This specifies the base Ubuntu system
BASEIMAGE_DATE="2020-03-12"
BASEIMAGE_UBUNTU_VERSION="10.3"
BASEIMAGE_URL=https://rcn-ee.com/rootfs/${BASEIMAGE_DATE}/microsd/bone-debian-${BASEIMAGE_UBUNTU_VERSION}-console-armhf-${BASEIMAGE_DATE}-2gb.img.xz
BASEIMAGE=`basename ${BASEIMAGE_URL}`

ANSIBLE_PLATFORM_VARS='{"platform":"replicape", "packaging":"apt"}'

function decompress {
	xz -c -d $BASEIMAGE >> $TARGETIMAGE
}
